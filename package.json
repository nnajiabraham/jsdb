{
  "name": "jsdb",
  "version": "1.0.0",
  "description": "A simple database created using TypeScript/JavaScript in Node.js",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "node --env-file=.env --watch -r ts-node/register src/index.ts",
    "test": "node --test --env-file=.env --watch -r ts-node/register **/*.test.ts",
    "test:file": "node --test --env-file=.env --watch -r ts-node/register \"src/${npm_config_folder}/*.test.ts\""
  },
  "keywords": [
    "database",
    "node",
    "typescript",
    "javascript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/sqlite3": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=22.8.0"
  },
  "dependencies": {
    "typed-binary": "^4.1.0"
  }
}
